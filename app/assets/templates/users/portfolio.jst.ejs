<% var headerString = ModelFindrApp.currentUserId === user.id ? "My Portfolio" : user.get('username') + "'s Portfolio" %>

<div class="portfolio-header">
  <h6><%= headerString %></h6>
  <button class="profile-btn">Profile</button>
  <% if (ModelFindrApp.isGuest !== true && ModelFindrApp.currentUserId === parseInt(user.id)) { %>
    <button class="upload-btn">Upload Photo</button>
  <% } %>
</div>

<div class="users-imgs">
</div>
