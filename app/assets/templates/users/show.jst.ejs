<div id="sidebar">
  <img id="avatar-img" src="<%= user.get('avatar_img') %>"/>
  <br>
  <h2>Details</h2>
  <ul id="user-details">
    <li> Compensation: <%= user.get('compensation') %></li>
    <br>
    <li> Experience: <%= user.get('experience') %></li>
  </ul>
</div>

<div class="user-general-info">
  <button id="edit-btn">Edit Profile</button>

  <h1><%= user.get('username') %></h1>
  <ul id="user-gen-info-list">
  </ul>
</div>

<div id="about-me">
  <h2>About Me</h2>
  <p>
    <%= user.get('about_me') %>
  </p>
</div>

<div id="portfolio">
  <h3>Portfolio</h3>
  <ul id="user-portfolio">
  </ul>
</div>
